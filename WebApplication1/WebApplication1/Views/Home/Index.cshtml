@model IEnumerable<WebApplication1.Models.ContactsModel.Contacts>
@{
    ViewData["Title"] = "Home Page";
}

<div class="table-intro">
    <h2>Contacts</h2>
    <p>These are the contacts available</p>
</div>
<a asp-area="" asp-controller="Home" asp-action="Add" class="btn btn-primary" id="btnAdd"><span class="glyphicon glyphicon-plus-sign"></span> Add Contacts</a>
<table class="table table-striped">
    <thread>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Number</th>
        </tr>
    </thread>
    <tbody>
        @foreach (WebApplication1.Models.ContactsModel.Contacts contacts in Model)
        {
            <tr>
                <td>@contacts.Fname</td>
                <td>@contacts.Lname</td>
                <td>@contacts.Num</td>
            </tr>
        }
    </tbody>
</table>